<mvc:View controllerName="ui5fragmentproject.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <VBox>
            <Text text="This is coming from View itself." />

            <!-- Lets attach Fragment here -->
            <core:Fragment fragmentName="ui5fragmentproject.view.fragment.ViewFragment" type="XML"/>

            <Button text="Call Dialog Fragment" press="callDialogFragment" />
            
        </VBox>
    </Page>
</mvc:View>