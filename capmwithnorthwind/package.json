{
  "name": "capmwithnorthwind",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "Northwind": {
        "kind": "odata",
        "model": "srv/external/Northwind",
        "[testing]":{
          "credentials":{
            "url":"https://services.odata.org/Experimental/OData/OData.svc"
          }
        },
        "[production]":{
          "credentials":{
            "destination":"Northwind_dest"
          }
        }
      }
    }
  }
}
